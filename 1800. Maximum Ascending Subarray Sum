class Solution {
    public int maxAscendingSum(int[] nums){
        int sum=nums[0];
        int maxsum=nums[0];
        for(int i=1;i<nums.length;i++){
            if(nums[i]>nums[i-1]) sum+=nums[i];
            else sum=nums[i];   //start another subarray from that number again
            maxsum=Math.max(maxsum, sum);
        }
        return maxsum;
    }
}
